{
  "name": "@rentpath/react-ui-packages",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "build:watch": "yarn run build -- --watch",
    "build": "lerna run build",
    "build-storybook": "build-storybook -c .storybook -s .storybook/dist -o build",
    "clean": "lerna run clean --yes",
    "lint": "yarn run lint:js && yarn run lint:css",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "stylelint \"**/*.css\" --fix",
    "lint:fix": "yarn run lint:js:fix && yarn run lint:css:fix",
    "lint:js": "eslint packages stories",
    "lint:js:fix": "lerna packages stories --fix",
    "postinstall": "lerna bootstrap",
    "prepublish": "lerna run clean --yes && lerna run build",
    "publish-packages": "lerna publish --exact --conventional-commits --registry=https://registry.npmjs.org/",
    "report-coverage": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "storybook": "start-storybook -p 6006 -c .storybook -s ./.storybook/dist",
    "test": "./node_modules/.bin/jest --coverage",
    "test:ci": "yarn run test --maxWorkers=2 && yarn run report-coverage",
    "test:coverage": "yarn run test && yarn run report-coverage"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/src/**/*.{js}",
      "!**/node_modules/**",
      "!**/index.js"
    ],
    "coverageDirectory": "./coverage",
    "projects": [
      "<rootDir>/packages/*"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@rentpath/eslint-config-rentpath": "3.0.1",
    "@storybook/addon-a11y": "^4.0.0",
    "@storybook/addon-actions": "^4.0.0",
    "@storybook/addon-info": "^4.0.0",
    "@storybook/addon-knobs": "^4.0.0",
    "@storybook/addon-links": "^4.0.0",
    "@storybook/addon-options": "^4.0.0",
    "@storybook/addon-viewport": "^4.0.0",
    "@storybook/addons": "^4.0.0",
    "@storybook/react": "^4.0.0",
    "@types/node": "^10.12.1",
    "@types/react": "^16.4.18",
    "@types/storybook__react": "^3.0.9",
    "autobind-decorator": "^2.1.0",
    "autoprefixer": "^9.3.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.4",
    "babel-plugin-dynamic-import-webpack": "^1.1.0",
    "commitizen": "^2.10.1",
    "coveralls": "^3.0.2",
    "css-loader": "^1.0.1",
    "cz-conventional-changelog": "2.1.0",
    "cz-lerna-changelog": "1.2.1",
    "eslint": "^5.8.0",
    "jest": "^23.6.0",
    "jest-css-modules": "1.1.0",
    "lerna": "^2.9.1",
    "node-sass": "^4.9.4",
    "postcss-loader": "^3.0.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-storybook-addon-chapters": "^2.1.9",
    "react-test-renderer": "^16.6.0",
    "sass-loader": "^7.1.0",
    "stylelint": "^9.7.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "classnames": "^2.2.6",
    "form-serialize": "0.7.2",
    "react-themed": "3.2.1"
  },
  "peerDependencies": {
    "react": "^16.6.0",
    "react-dom": "^16.6.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-lerna-changelog"
    }
  }
}
