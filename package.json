{
  "name": "@rentpath/react-ui-packages",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "build": "lerna run build",
    "build-storybook": "build-storybook -c .storybook -s .storybook/dist -o build",
    "clean": "lerna run clean --yes",
    "lint": "yarn run lint:js && yarn run lint:css",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "stylelint \"**/*.css\" --fix",
    "lint:fix": "yarn run lint:js:fix && yarn run lint:css:fix",
    "lint:js": "eslint packages stories",
    "lint:js:fix": "lerna packages stories --fix",
    "postinstall": "lerna bootstrap",
    "prepublish": "lerna run clean --yes && lerna run build",
    "publish-packages": "lerna publish --exact --conventional-commits --registry=https://registry.npmjs.org/",
    "report-coverage": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "storybook": "start-storybook -p 6006 -c .storybook -s ./.storybook/dist",
    "test": "./node_modules/.bin/jest --coverage",
    "test:ci": "./node_modules/.bin/jest --coverage --maxWorkers=2 && yarn run report-coverage",
    "test:coverage": "yarn run test && yarn run report-coverage"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/src/**/*.{js}",
      "!**/node_modules/**",
      "!**/index.js"
    ],
    "coverageDirectory": "./coverage",
    "projects": [
      "packages/react-ui-core",
      "packages/react-ui-tracking",
      "packages/react-ui-utils",
      "packages/react-ui-ag",
      "packages/react-ui-rent",
      "packages/react-ui-rentals"
    ]
  },
  "devDependencies": {
    "@rentpath/eslint-config-rentpath": "3.0.1",
    "@storybook/addon-a11y": "3.3.10",
    "@storybook/addon-actions": "3.3.10",
    "@storybook/addon-info": "3.3.10",
    "@storybook/addon-knobs": "3.3.10",
    "@storybook/addon-options": "3.3.10",
    "@storybook/addon-viewport": "3.3.10",
    "@storybook/addons": "^3.3.10",
    "@storybook/react": "3.3.10",
    "@types/node": "9.3.0",
    "@types/react": "16.0.34",
    "@types/storybook__react": "3.0.6",
    "autoprefixer": "7.2.5",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.1",
    "babel-jest": "22.1.0",
    "babel-plugin-dynamic-import-webpack": "1.0.2",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "commitizen": "2.9.6",
    "coveralls": "3.0.0",
    "css-loader": "0.28.9",
    "cz-conventional-changelog": "2.1.0",
    "cz-lerna-changelog": "1.2.1",
    "eslint": "4.15.0",
    "jest": "22.1.4",
    "jest-css-modules": "1.1.0",
    "lerna": "2.8.0",
    "postcss-loader": "2.0.10",
    "prop-types": "15.6.0",
    "react": "16.2.0",
    "react-dom": "16.2.0",
    "react-storybook-addon-chapters": "2.1.1",
    "react-test-renderer": "16.2.0",
    "stylelint": "8.4.0"
  },
  "dependencies": {
    "babel-runtime": "6.26.0",
    "classnames": "2.2.5",
    "form-serialize": "0.7.2",
    "react-themed": "3.2.1"
  },
  "peerDependencies": {
    "react": "^16.2.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-lerna-changelog"
    }
  }
}
