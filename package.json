{
  "name": "@rentpath/react-ui-packages",
  "version": "0.0.1",
  "main": "packages/react-ui-core/lib/index.js",
  "license": "MIT",
  "scripts": {
    "build:watch": "yarn run build -- --watch",
    "build": "lerna run build",
    "build-storybook": "build-storybook -c .storybook -s .storybook/dist -o build",
    "clean": "lerna run clean --yes",
    "lint": "yarn run lint:js && yarn run lint:css",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "stylelint \"**/*.css\" --fix",
    "lint:fix": "yarn run lint:js:fix && yarn run lint:css:fix",
    "lint:js": "eslint packages stories",
    "lint:js:fix": "lerna packages stories --fix",
    "postinstall": "lerna bootstrap",
    "prepublish": "lerna run clean --yes && lerna run build",
    "publish-packages": "lerna publish --exact --conventional-commits --registry=https://registry.npmjs.org/",
    "report-coverage": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "storybook": "start-storybook -p 6006 -c .storybook -s ./.storybook/dist",
    "test": "./node_modules/.bin/jest --coverage",
    "test:ci": "yarn run test --maxWorkers=2 && yarn run report-coverage",
    "test:coverage": "yarn run test && yarn run report-coverage"
  },
  "jest": {
    "collectCoverageFrom": [
      "**/src/**/*.{js}",
      "!**/node_modules/**",
      "!**/index.js"
    ],
    "coverageDirectory": "./coverage",
    "projects": [
      "packages/react-ui-core",
      "packages/react-ui-tracking",
      "packages/react-ui-utils",
      "packages/react-ui-ag",
      "packages/react-ui-rent",
      "packages/react-ui-rentals"
    ]
  },
  "devDependencies": {
    "@rentpath/eslint-config-rentpath": "3.0.1",
    "@storybook/addon-a11y": "^3.4.7",
    "@storybook/addon-actions": "^3.4.7",
    "@storybook/addon-info": "^3.4.7",
    "@storybook/addon-knobs": "^3.4.7",
    "@storybook/addon-options": "^3.4.7",
    "@storybook/addon-viewport": "^3.4.7",
    "@storybook/addons": "^3.4.7",
    "@storybook/react": "^3.4.7",
    "@types/node": "^10.3.4",
    "@types/react": "^16.4.0",
    "@types/storybook__react": "^3.0.7",
    "autobind-decorator": "^2.1.0",
    "autoprefixer": "^8.6.3",
    "babel-cli": "6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "babel-plugin-dynamic-import-webpack": "1.0.2",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "6.24.1",
    "commitizen": "^2.10.1",
    "coveralls": "^3.0.1",
    "css-loader": "^0.28.11",
    "cz-conventional-changelog": "2.1.0",
    "cz-lerna-changelog": "1.2.1",
    "eslint": "^4.19.1",
    "jest": "^22.4.3",
    "jest-css-modules": "1.1.0",
    "lerna": "^2.9.1",
    "postcss-loader": "^2.1.5",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-storybook-addon-chapters": "^2.1.5",
    "react-test-renderer": "^16.4.1",
    "stylelint": "^9.3.0"
  },
  "dependencies": {
    "babel-runtime": "6.26.0",
    "classnames": "^2.2.6",
    "form-serialize": "0.7.2",
    "react-themed": "3.2.1"
  },
  "peerDependencies": {
    "react": "^16.3.0"
  },
  "config": {
    "commitizen": {
      "path": "cz-lerna-changelog"
    }
  }
}
