{
  "name": "@rentpath/react-ui-ag",
  "version": "11.3.3",
  "main": "lib/index.js",
  "module": "es/index.js",
  "sideEffects": false,
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf lib es",
    "build": "yarn run build:es && yarn run build:cjs",
    "build:cjs": "BABEL_ENV=cjs babel --ignore __tests__ -d lib src",
    "build:cjs:watch": "yarn run build:cjs -- --watch",
    "build:es": "BABEL_ENV=es babel --ignore __tests__ -d es src",
    "build:es:watch": "yarn run build:es -- --watch",
    "build:watch": "yarn run build -- --watch",
    "test": "NODE_ENV=test jest",
    "test:watch": "yarn run test -- --watch",
    "lint": "eslint src",
    "lint:fix": "yarn run lint -- --fix"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/test/shim.js",
      "<rootDir>/test/setup.js"
    ],
    "moduleNameMapper": {
      "\\.(css)$": "<rootDir>/node_modules/jest-css-modules"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/mocks/"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@rentpath/eslint-config-rentpath": "3.0.1",
    "autobind-decorator": "^2.1.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.4.2",
    "babel-plugin-react-css-modules": "^3.4.2",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint": "^5.8.0",
    "jest": "^23.6.0",
    "jest-css-modules": "1.1.0",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-test-renderer": "^16.6.0"
  },
  "dependencies": {
    "@rentpath/react-ui-core": "16.3.3",
    "@rentpath/react-ui-utils": "4.2.3",
    "react-lazyload": "^2.3.0"
  },
  "peerDependencies": {
    "classnames": "^2.2.6",
    "prop-types": "^15.6.2",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-themed": "^3.2.1"
  }
}
