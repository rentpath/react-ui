{
  "env": {
    "es": {
      "presets": [
        ["env", {
          "modules": false,
          "targets": {
            "browsers": [">0.25%", "not op_mini all"]
          }
        }],
        "react"
      ],
      "plugins": [
        "transform-runtime",
        "transform-decorators-legacy",
        "transform-class-properties",
        "babel-plugin-transform-object-rest-spread",
        "dynamic-import-webpack"
      ]
    },
    "cjs": {
      "presets": [
        "env",
        "react"
      ],
      "plugins": [
        "transform-runtime",
        "transform-decorators-legacy",
        "transform-class-properties",
        "babel-plugin-transform-object-rest-spread",
        "dynamic-import-webpack"
      ]
    },
    "test": {
      "presets":[
        "env",
        "react",
        "stage-0"
      ],
      "plugins": [
        "transform-es2015-modules-commonjs",
        "transform-decorators-legacy",
        "dynamic-import-node"
      ]
    }
  }
}
