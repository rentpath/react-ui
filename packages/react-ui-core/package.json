{
  "name": "@rentpath/react-ui-core",
  "version": "1.0.2",
  "main": "lib/index.js",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf lib",
    "build": "babel --ignore __tests__ -d lib src",
    "build:watch": "yarn run build -- --watch",
    "test": "jest",
    "test:watch": "yarn run test -- --watch",
    "lint": "eslint src",
    "lint:fix": "yarn run lint -- --fix"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/test/shim.js",
      "<rootDir>/test/setup.js"
    ],
    "moduleNameMapper": {
      "\\.(css)$": "<rootDir>/node_modules/jest-css-modules"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/mocks/"
    ]
  },
  "devDependencies": {
    "@rentpath/eslint-config-rentpath": "^3.0.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-plugin-react-css-modules": "^3.3.1",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "eslint": "^4.8.0",
    "jest": "^21.2.1",
    "jest-css-modules": "^1.1.0",
    "react-dom": "^16.0.0",
    "react-input-range": "1.1.4",
    "react-test-renderer": "^16.0.0"
  },
  "dependencies": {
    "@rentpath/react-ui-utils": "0.0.1",
    "babel-runtime": "^6.26.0",
    "classnames": "^2.2.5",
    "form-serialize": "^0.7.1",
    "prop-types": "^15.6.0",
    "react-flexbox-grid": "https://github.com/rentpath/react-flexbox-grid",
    "react-input-range": "^1.2.1",
    "react-themed": "^3.2.1"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  }
}
