{
  "name": "@rentpath/react-ui-core",
  "version": "4.1.0",
  "main": "lib/index.js",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf lib",
    "build": "NODE_ENV=production babel --ignore __tests__ -d lib src",
    "build:watch": "yarn run build -- --watch",
    "test": "jest",
    "test:watch": "yarn run test -- --watch",
    "lint": "eslint src",
    "lint:fix": "yarn run lint -- --fix"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/test/shim.js",
      "<rootDir>/test/setup.js"
    ],
    "moduleNameMapper": {
      "\\.(css)$": "<rootDir>/node_modules/jest-css-modules"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/mocks/"
    ]
  },
  "devDependencies": {
  },
  "dependencies": {
    "@rentpath/react-ui-utils": "1.0.2",
    "classnames": "2.2.5",
    "form-serialize": "0.7.2",
    "mapbox-gl": "0.42.2",
    "prop-types": "15.6.0",
    "react-flexbox-grid": "https://github.com/rentpath/react-flexbox-grid",
    "react-image-gallery": "0.8.7",
    "react-input-range": "1.3.0",
    "react-lazyload": "2.3.0",
    "react-motion": "^0.5.2",
    "react-themed": "3.2.1"
  },
  "peerDependencies": {
    "react": "^16.2.0"
  }
}
