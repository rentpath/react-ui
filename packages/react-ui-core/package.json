    "@types/react": "^16.0.40",
    "@types/react-dom": "^16.0.4",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.1",
    "babel-jest": "22.1.0",
    "babel-plugin-dynamic-import-webpack": "^1.0.2",
    "babel-plugin-react-css-modules": "3.3.3",
