{
  "name": "@rentpath/react-ui-core",
  "version": "0.0.33",
  "main": "lib/index.js",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf lib",
    "build": "babel --ignore __tests__ -d lib src",
    "build:watch": "yarn run build -- --watch",
    "test": "jest",
    "test:watch": "yarn run test -- --watch",
    "lint": "eslint src",
    "lint:fix": "yarn run lint -- --fix"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css)$": "<rootDir>/node_modules/jest-css-modules"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/mocks/"
    ]
  },
  "devDependencies": {
    "@rentpath/eslint-config-rentpath": "^3.0.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.0",
    "babel-jest": "^20.0.3",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "enzyme": "^2.7.1",
    "eslint": "^4.5.0",
    "jest": "^20.0.4",
    "jest-css-modules": "^1.1.0",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-input-range": "1.1.4",
    "react-test-renderer": "^15.6.1"
  },
  "dependencies": {
    "@rentpath/react-ui-utils": "0.0.1",
    "babel-plugin-react-css-modules": "^3.0.1",
    "babel-runtime": "^6.23.0",
    "classnames": "^2.2.5",
    "form-serialize": "^0.7.1",
    "prop-types": "^15.5.10",
    "react-flexbox-grid": "https://github.com/rentpath/react-flexbox-grid",
    "react-input-range": "1.1.4",
    "react-themed": "3.2.0"
  },
  "peerDependencies": {
    "react": "^15.4.2"
  }
}
