{
  "name": "@rentpath/react-ui-core",
  "version": "16.1.0",
  "main": "lib/index.js",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf lib",
    "build": "NODE_ENV=production babel --ignore __tests__ -d lib src",
    "build:watch": "yarn run build -- --watch",
    "test": "jest",
    "test:watch": "yarn run test -- --watch",
    "lint": "eslint src",
    "lint:fix": "yarn run lint -- --fix"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/test/shim.js",
      "<rootDir>/test/setup.js"
    ],
    "moduleNameMapper": {
      "\\.(css)$": "<rootDir>/node_modules/jest-css-modules"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/mocks/"
    ]
  },
  "devDependencies": {
    "@rentpath/eslint-config-rentpath": "3.0.1",
    "babel-cli": "6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-dynamic-import-node": "^2.1.0",
    "babel-plugin-dynamic-import-webpack": "^1.0.2",
    "babel-plugin-react-css-modules": "^3.4.2",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "6.24.1",
    "date-fns": "2.0.0-alpha.16",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "^5.5.0",
    "jest": "^23.6.0",
    "jest-css-modules": "1.1.0",
    "react-test-renderer": "^16.4.1"
  },
  "dependencies": {
    "@rentpath/react-ui-utils": "4.2.0",
    "babel-runtime": "6.26.0",
    "classnames": "^2.2.6",
    "form-serialize": "0.7.2",
    "prop-types": "^15.6.1",
    "react-image-gallery": "rentpath/react-image-gallery",
    "react-input-mask": "^2.0.3",
    "react-input-range": "1.3.0",
    "react-lazyload": "2.3.0",
    "react-motion": "^0.5.2",
    "react-themed": "3.2.1"
  },
  "peerDependencies": {
    "date-fns": "^2.0.0",
    "lodash": "^4.17.10",
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  }
}
