{
  "name": "@rentpath/react-ui-core",
  "version": "19.4.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/rentpath/react-ui.git",
    "directory": "packages/react-ui-core"
  },
  "main": "lib/index.js",
  "module": "es/index.js",
  "license": "MIT",
  "sideEffects": false,
  "files": [
    "es/",
    "lib/"
  ],
  "scripts": {
    "clean": "rm -rf lib es",
    "build": "yarn run build:es && yarn run build:cjs",
    "build:cjs": "BABEL_ENV=cjs babel -d lib src",
    "build:cjs:watch": "yarn run build:cjs --watch --verbose",
    "build:es": "BABEL_ENV=es babel -d es src",
    "build:es:watch": "yarn run build:es --watch --verbose",
    "test": "jest",
    "test:watch": "yarn run test --watch",
    "lint": "eslint src",
    "lint:fix": "yarn run lint --fix"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/test/shim.js",
      "<rootDir>/test/setup.js"
    ],
    "moduleNameMapper": {
      "\\.(css)$": "<rootDir>/node_modules/jest-css-modules"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/mocks/",
      "./es",
      "./lib",
      "^.*__tests__/__helpers__.*"
    ]
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "@rentpath/react-ui-utils": "4.2.11",
    "form-serialize": "0.7.2",
    "react-image-gallery": "rentpath/react-image-gallery#0.8.9",
    "react-input-mask": "2.0.4",
    "react-input-range": "1.3.0",
    "react-lazyload": "2.3.0"
  },
  "peerDependencies": {
    "@rentpath/react-themed": "^4.1.0",
    "clsx": "*",
    "date-fns": "^2.0.0",
    "lodash": "^4.17.10",
    "prop-types": "15.7.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  }
}
