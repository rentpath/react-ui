{
  "env": {
    "production": {
      "presets": [
        "es2015",
        "react"
      ],
      "plugins": [
        "transform-runtime",
        "transform-decorators-legacy",
        "transform-class-properties",
        "babel-plugin-transform-object-rest-spread",
        "dynamic-import-webpack"
      ]
    },
    "test": {
      "presets":[
        "es2015",
        "react",
        "stage-0"
      ],
      "plugins": [
        "transform-es2015-modules-commonjs",
        "transform-decorators-legacy",
        "dynamic-import-node"
      ]
    }
  }
}
