{
  "name": "@rentpath/react-ui-utils",
  "version": "4.2.5",
  "repository": "https://github.com/rentpath/react-ui/tree/master/packages/react-ui-utils",
  "main": "lib/index.js",
  "module": "es/index.js",
  "sideEffects": false,
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf lib es",
    "build": "yarn run build:es && yarn run build:cjs",
    "build:cjs": "BABEL_ENV=cjs babel --ignore __tests__ -d lib src",
    "build:cjs:watch": "yarn run build:cjs -- --watch",
    "build:es": "BABEL_ENV=es babel --ignore __tests__ -d es src",
    "build:es:watch": "yarn run build:es -- --watch",
    "build:watch": "yarn run build -- --watch",
    "test": "jest",
    "test:watch": "yarn run test -- --watch",
    "lint": "eslint src",
    "lint:fix": "yarn run lint -- --fix"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/mocks/",
      "./es",
      "./lib"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "babel-core": "^7.0.0-bridge",
    "babel-jest": "^23.6.0",
    "eslint": "^5.6.0",
    "jest": "^23.6.0",
    "jsverify": "^0.8.3",
    "react": "^16.6.0"
  },
  "peerDependencies": {
    "classnames": "^2.2.6"
  }
}
