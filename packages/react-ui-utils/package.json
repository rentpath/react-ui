{
  "name": "@rentpath/react-ui-utils",
  "version": "4.2.2",
  "main": "lib/index.js",
  "module": "es/index.js",
  "sideEffects": false,
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf lib es",
    "build": "yarn run build:es && yarn run build:cjs",
    "build:cjs": "BABEL_ENV=cjs babel --ignore __tests__ -d lib src",
    "build:cjs:watch": "yarn run build:cjs -- --watch",
    "build:es": "BABEL_ENV=es babel --ignore __tests__ -d es src",
    "build:es:watch": "yarn run build:es -- --watch",
    "build:watch": "yarn run build -- --watch",
    "test": "NODE_ENV=test jest",
    "test:watch": "yarn run test -- --watch",
    "lint": "eslint src",
    "lint:fix": "yarn run lint -- --fix"
  },
  "devDependencies": {
    "@rentpath/eslint-config-rentpath": "3.0.1",
    "babel-cli": "6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.6.0",
    "jest": "^23.6.0",
    "jsverify": "^0.8.3",
    "react": "^16.6.0"
  },
  "peerDependencies": {
    "classnames": "^2.2.6"
  }
}
