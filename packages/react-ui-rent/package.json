{
  "name": "@rentpath/react-ui-rent",
  "version": "4.3.4",
  "main": "lib/index.js",
  "module": "es/index.js",
  "sideEffects": false,
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf lib es",
    "build": "yarn run build:es && yarn run build:cjs",
    "build:cjs": "BABEL_ENV=cjs babel --ignore __tests__ -d lib src",
    "build:cjs:watch": "yarn run build:cjs -- --watch",
    "build:es": "BABEL_ENV=es babel --ignore __tests__ -d es src",
    "build:es:watch": "yarn run build:es -- --watch",
    "build:watch": "yarn run build -- --watch",
    "test": "NODE_ENV=test jest",
    "test:watch": "yarn run test -- --watch",
    "lint": "eslint src",
    "lint:fix": "yarn run lint -- --fix"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/test/shim.js",
      "<rootDir>/test/setup.js"
    ],
    "moduleNameMapper": {
      "\\.(css)$": "<rootDir>/node_modules/jest-css-modules"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/mocks/"
    ]
  },
  "devDependencies": {
    "@rentpath/eslint-config-rentpath": "3.0.1",
    "babel-cli": "6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-react-css-modules": "^3.4.2",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "6.24.1",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "^5.6.0",
    "jest": "^23.6.0",
    "jest-css-modules": "1.1.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-test-renderer": "^16.5.2"
  },
  "dependencies": {
    "@rentpath/react-ui-core": "16.3.4",
    "@rentpath/react-ui-utils": "4.2.4"
  },
  "peerDependencies": {
    "classnames": "^2.2.6",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-themed": "^3.2.1"
  }
}
