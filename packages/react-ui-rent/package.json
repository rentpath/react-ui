{
  "name": "@rentpath/react-ui-rent",
  "version": "0.3.6",
  "main": "lib/index.js",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf lib",
    "build": "babel --ignore __tests__ -d lib src",
    "build:watch": "yarn run build -- --watch",
    "test": "NODE_ENV=test jest",
    "test:watch": "yarn run test -- --watch",
    "lint": "eslint src",
    "lint:fix": "yarn run lint -- --fix"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/test/shim.js",
      "<rootDir>/test/setup.js"
    ],
    "moduleNameMapper": {
      "\\.(css)$": "<rootDir>/node_modules/jest-css-modules"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/mocks/"
    ]
  },
  "devDependencies": {
  },
  "dependencies": {
    "@rentpath/react-ui-core": "4.1.0",
    "@rentpath/react-ui-utils": "1.0.2",
    "classnames": "2.2.5",
    "prop-types": "15.6.0",
    "react-themed": "3.2.1"
  },
  "peerDependencies": {
    "react": "^16.2.0"
  }
}
