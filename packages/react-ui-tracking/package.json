{
  "name": "@rentpath/react-ui-tracking",
  "version": "4.2.6",
  "repository": "https://github.com/rentpath/react-ui/tree/master/packages/react-ui-tracking",
  "main": "lib/index.js",
  "module": "es/index.js",
  "sideEffects": false,
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf lib es",
    "build": "yarn run build:es && yarn run build:cjs",
    "build:cjs": "BABEL_ENV=cjs babel --ignore __tests__ -d lib src",
    "build:cjs:watch": "yarn run build:cjs -- --watch",
    "build:es": "BABEL_ENV=es babel --ignore __tests__ -d es src",
    "build:es:watch": "yarn run build:es -- --watch",
    "build:watch": "yarn run build -- --watch",
    "test": "jest",
    "test:watch": "yarn run test -- --watch",
    "lint": "eslint src",
    "lint:fix": "yarn run lint -- --fix"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/test/setup.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/mocks/",
      "./es",
      "./lib"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@rentpath/eslint-config-rentpath": "^3.0.1",
    "babel-core": "^7.0.0-bridge",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "classnames": "^2.2.6",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "eslint": "^5.5.0",
    "jest": "^23.6.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.1",
    "react-dom": "^16.6.1",
    "react-test-renderer": "^16.6.1"
  },
  "peerDependencies": {
    "prop-types": "^15.6.2",
    "react": "^16.6.1",
    "react-dom": "^16.6.1"
  }
}
